/*
ZCOSCH PROBLEM
*/



class Codechef{
	public static void main(String[] args) throws IOException {
		Scanner sc = new Scanner(System.in);
		long r = sc.nextLong();
		if(r>=1 && r<=50){
		    System.out.println(100);
		}
		else if(r>=51 && r<=100){
		    System.out.println(50);
		}
		else{
		    System.out.println(0);
		}
 }
}
