/*Packaging Cupcakes*/
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t!=0){
		    int n = s.nextInt();
		    int val = (n/2)+1;
		  
		    System.out.println(val);
		    
		    t--;
		}
	}
}
